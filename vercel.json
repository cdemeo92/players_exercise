{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/dist/main.js" },
    { "src": "/swagger-ui.css", "dest": "/dist/swagger-ui/swagger-ui.css" },
    { "src": "/swagger-ui-bundle.js", "dest": "/dist/swagger-ui/swagger-ui-bundle.js" },
    { "src": "/swagger-ui-standalone-preset.js", "dest": "/dist/swagger-ui/swagger-ui-standalone-preset.js" }
  ]
}